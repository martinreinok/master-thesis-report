@article{sim_of_suscept_artifact,
title = {Simulation of susceptibility artifacts in 2D and 3D fourier transform spin-echo and gradient-echo magnetic resonance imaging},
journal = {Magnetic Resonance Imaging},
volume = {12},
number = {5},
pages = {767-774},
year = {1994},
issn = {0730-725X},
doi = {https://doi.org/10.1016/0730-725X(94)92201-2},
url = {https://www.sciencedirect.com/science/article/pii/0730725X94922012},
author = {C.J.G. Bakker and R. Bhagwandien and M.A. Moerland and L.M.P. Ramos},
keywords = {Magnetic resonance imaging, Susceptibility artifacts, Geometric distortion,  effects},
abstract = {A method is presented for simulating susceptibility artifacts in 2D and 3D spin-echo and gradient-echo imaging of arbitrary susceptibility distributions. The method incorporates object induced field perturbations in the time domain (k-space) and is demonstrated for spherical nonuniformities in susceptibility. Both simulations and experimental results for multi-slice 2D and 3D imaging of phantoms and human subjects are provided.}
}

